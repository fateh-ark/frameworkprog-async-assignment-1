<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="excercise_1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:excercise_1"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Main Page"
        ContentTemplate="{DataTemplate local:MainPage}"
     />

    <ShellContent
        Title="Simple MVVM"
        ContentTemplate="{DataTemplate local:SimpleMVVM}"
     />

    <ShellContent
        Title="Interactive MVVM"
        ContentTemplate="{DataTemplate local:InteractiveMVVM}"
     />

    <ShellContent
        Title="Commanding"
        ContentTemplate="{DataTemplate local:Comanding}"
     />

</Shell>
